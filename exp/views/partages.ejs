<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="css/site.css">

    <title>vos partages</title>
  </head>
  <body>
  <div class="wrapper">
      
      <nav class="navbar navbar-expand-lg navbar-light bg-dark">
  <a class="navbar-brand" href="#">centre de partage</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item active">
        <a class="nav-link" href="/home">Home <span class="sr-only">(current)</span></a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/home">all products</a>
      </li>
      <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
          categories
        </a>
        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
          <a class="dropdown-item" href="/vetements">vetements</a>
          <a class="dropdown-item" href="/alimentations">alimentation</a>
          <a class="dropdown-item" href="/vehicules">v√©hicules</a>
          <a class="dropdown-item" href="/meubles">meubles</a>
        </div>
      </li>
    </ul>
   
    <ul class="user">
     <li class="nav-item dropdown ">
        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
          <%=username%>
        </a>
        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
          <a class="dropdown-item" href="/achats">vos achats</a>
          <a class="dropdown-item" href="/favoris">favoris</a>
           <a class="dropdown-item" href="/logout">logout</a>
        </div>
      </li>
    </ul>
   </div>
</nav>
    <div class="header"></div>
        <div class="section">
          <div class="row">
              <div class="col-lg-9 col-md-9 col-sm-12 col-xs 12  products_section"> <h1>vos partages</h1>
              
               <% if(content.length==0) { %>
               <p style="text-align:center;">vous n'avez fait aucun partage</p>
               <% }else{%>
               
              <ul class="products_list">
                  
                 <% for(var i=0; i < content.length; i++) { %>
                     <li>
                       <span class="nom"> <%= content[i]["nom_objet"] %></span>
                       <span class="prix"><%=content[i]["prix"]+"$"%></span>
                       <span class="date_fin"><a class="btn" data-toggle="collapse" data-target=<%="#viewdetail"+i %>>View details &raquo;</a></span>
                     </li>
                       <div id=<%="viewdetail"+i %> class="collapse">
                         <p><span>date du partage :<%=content[i]["date_annonce"] %></span></p>   
                       <p><span>date d'achats :<%=content[i]["date_achat"] %></span></p>
                       <% if(content[i]["date_fin"]) { %>
                       <p><span>date fin de l'annonce :<%=content[i]["date_fin"] %></span></p>
                       <% }%>
                       
                      </div>
                  <% } %>
                     </ul>
                     <% } %>
              </div>
          </div>
          
      </div>
      
  </div>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
  </body>
</html>